package simulator.ISA.R;

import simulator.ISA.RTypeInstruction;

public class ISA_Add extends RTypeInstruction {

	public ISA_Add(){
		this.mOpCode = "ADD";
		this.mFunct = 32;
	}
	
	public void TEST_Populate(){
		this.mFetchedRD = 1;
		this.mFetchedRS = 2;
		this.mFetchedRT = 3;
		this.mShamt = 0;
	}
	
}